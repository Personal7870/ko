attractions.html
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="./bootstrap-5.3.3-dist/css/bootstrap.css">
    <link rel="stylesheet" href="./bootstrap-5.3.3-dist/js/bootstrap.js">
    <script src="./jquery-3.7.1.min.js"></script>
</head>
<body>
    <table class="table table-stripped" id="tb1">
        <thead>
         <tr>
           <th scope="col">Ride name</th>
           <th scope="col">Age required</th>
           <th scope="col">Minimum Height</th>
           <th scope="col">Image</th>
         </tr>
        </thead>
    </table>

    <script>
        $(document).ready(function(){
            $.getJSON("./data.json",function(data){
                var std = ' ';
                $.each(data,function(key,value){
                    std += '<tbody>';
                        std += '<tr>';
                            std += '<td>' + value.ridename + '</td>';
                            std += '<td>' + value.agerequired + '</td>';
                            std += '<td>' + value.minimumheight + '</td>';
                            std += '<td> <img src=\"' + value.img + '\" alt="" height="100px" width="100px"> </td>';
                            std += '</tr>';
                            std += '</tbody>';
                });
                $('#tb1').append(std);
            });
        });
    </script>
    <center><br><br><a href="index.html"><input type="button" value="home"><br></a></center>
</body>
</html>

data.json
[
    {
      "ridename" : "Dare to Death",
      "agerequired" : 18,
      "minimumheight" :"165cm",
      "img" :"./img.png"
    }
]
