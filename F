style.css
body {
    font-family: Arial, Helvetica, sans-serif;
    background-color: rgb(30, 0, 139);
    margin: 0;
    padding: 0;
}

.container {
    max-width: 400px;
    margin: 100px auto;
    padding: 20px; 
    background-color: hsla(0, 0%, 100%, 0.577);
    border-radius: 50px;
    
}

form {
    display: flexbox;
    flex-direction: row;
}

h2{
    text-align: center;
    margin-bottom: 50px;
}

.input-container {
    margin-bottom: 15px;
}

label {
    font-weight: bold;
}

input[type="username"]
input[type="email"]
input[type="password"]{
    width: 200px;
    padding: 10px;
    border: 1px solid #ccc;
    border-radius: 3px;
}


button{
    background-color: #000000;
    color: #fff;
    padding: 10px;
    border: none;
    border-radius: 3px;
    cursor: pointer;
}

button:hover {
    background-color: #e34e09;
}

